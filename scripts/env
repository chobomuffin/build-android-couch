# Important paths for Android binaries

export COUCH_ANDROID_HOME=$(pwd)
export ANDROID_APP_NAME="com.test.couch"

paths="
$(pwd)/scripts
$(pwd)/build/deps/android/other/apk
$(pwd)/build/deps/android/prebuilt/linux-x86/toolchain/arm-eabi-4.4.0/bin/
"

for i in $paths
do
    if [[ -d $i ]]; then
        PATH=$PATH:$i
    fi
done

export PATH